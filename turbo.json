{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "lint"],
      "outputs": ["dist/**", ".next/**"]
    },
    "start": {
      "dependsOn": ["build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {},
    "format": {}
  }
}
